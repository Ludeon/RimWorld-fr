<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Moralist -->

  <RulePackDef>
    <defName>NamerRoleMoralist</defName>
    <rulePack>
      <rulesStrings>
        <li>r_roleName(p=4)->[memeMoralist]</li>
        <li>r_roleName(p=2)->[memeAdjective] [memeMoralist]</li>
        <li>r_roleName->[memeMoralist] of [chosenTheme]</li>
        <li>r_roleName->[memeMoralist] of [keyDeity]</li>
        <li>r_roleName->[memeMoralist] of [memeConcept]</li>
        <li>r_roleName->[memeMoralist] of [chosenIdeoName]</li>
        <li>r_roleName->[chosenAdjective] [memeMoralist]</li>
        <li>r_roleName->[keyDeity]'s [memeMoralist]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

