<?xml version="1.0" encoding="utf-8" ?>
<Defs>  

    <!-- Small walkable buildings -->
    <ThingDef Name="AncientSmallWalkableBuildingBase" ParentName="BuildingBase" Abstract="True">
      <graphicData>
        <graphicClass>Graphic_Single</graphicClass>
        <drawSize>(1,1)</drawSize>
      </graphicData>
      <size>(1,1)</size>
      <fillPercent>0.5</fillPercent>
      <altitudeLayer>Building</altitudeLayer>
      <rotatable>false</rotatable>
      <passability>PassThroughOnly</passability>
      <pathCost>50</pathCost>
      <statBases>
        <MaxHitPoints>150</MaxHitPoints>
        <WorkToBuild>30</WorkToBuild>
        <Flammability>0</Flammability>
      </statBases>
    </ThingDef>
  
    <ThingDef ParentName="AncientSmallWalkableBuildingBase">
      <defName>AncientShoppingCart</defName>
      <label>ancient shopping cart</label>
      <description>An ancient, rusted shopping cart. How exactly it got here is a mystery, but the story is long and probably quite sad.</description>
      <graphicData>
        <texPath>Things/Building/Ruins/ShoppingCart</texPath>
        <shadowData>
          <volume>(0.9, 0.35, 0.45)</volume>
        </shadowData>
      </graphicData>
      <statBases>
        <MaxHitPoints>150</MaxHitPoints>
        <WorkToBuild>30</WorkToBuild>
      </statBases>
    </ThingDef>
  
