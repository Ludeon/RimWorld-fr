<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef Abstract="True" Name="BuildingFloorCoveringBase" ParentName="BuildingBase">
    <altitudeLayer>FloorCoverings</altitudeLayer>
    <blockPlants>true</blockPlants>
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsFurniture</li>
    </thingCategories>
    <designationCategory>Floors</designationCategory>
    <selectable>true</selectable>
    <useHitPoints>true</useHitPoints>
    <defaultPlacingRot>South</defaultPlacingRot>
    <canGenerateDefaultDesignator>false</canGenerateDefaultDesignator>
    <blocksAltitudes>
      <li>FloorCoverings</li>
    </blocksAltitudes>
    <building>
      <isEdifice>false</isEdifice>
    </building>
  </ThingDef>

  <ThingDef Abstract="True" Name="BuildingFloorCoveringMediumBase" ParentName="BuildingFloorCoveringBase">
    <size>(3,3)</size>
    <statBases>
      <Beauty>15</Beauty>
      <WorkToBuild>1000</WorkToBuild>
      <Mass>100</Mass>
      <StyleDominance>30</StyleDominance>
    </statBases>
  </ThingDef>

  <ThingDef Abstract="True" Name="BuildingFloorCoveringBroadBase" ParentName="BuildingFloorCoveringBase">
    <size>(4,4)</size>
    <statBases>
      <Beauty>30</Beauty>
      <WorkToBuild>2000</WorkToBuild>
      <Mass>150</Mass>
      <StyleDominance>40</StyleDominance>
    </statBases>
  </ThingDef>

  <ThingDef ParentName="BuildingFloorCoveringMediumBase">
    <defName>MorbidSlab_Medium</defName>
    <label>morbid slab (medium)</label>
    <description>A stone bas relief of morbid imagery for decorating a floor.</description>
    <dominantStyleCategory>Morbid</dominantStyleCategory>
    <graphicData>
      <graphicClass>Graphic_Random</graphicClass>
      <texPath>Things/Building/FloorCoverings/SlabMorbidMedium</texPath>
      <drawSize>(3,3)</drawSize>
    </graphicData>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <stuffCategories>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>100</costStuffCount>
    <researchPrerequisites>
      <li>Stonecutting</li>
    </researchPrerequisites>
  </ThingDef>

